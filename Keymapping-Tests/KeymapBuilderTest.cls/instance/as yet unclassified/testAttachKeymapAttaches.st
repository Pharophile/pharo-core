testAttachKeymapAttaches	KMFactory keymapContainer		initializeKeymap: #test		executingOn: $r ctrl, $r asKeyCombination, $r asKeyCombination		doing: [ :receiver | "nothing" ]		inCategory: #Testing		platform: #all..	KMFactory keymapContainer attachCategoryName: #Testing to: KMMockMorph.		self assert: ((KMFactory keymapContainer categoriesForClass: KMMockMorph ) anySatisfy: [ :c | c name = #Testing ]).