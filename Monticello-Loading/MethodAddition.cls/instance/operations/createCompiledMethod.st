createCompiledMethod	compiledMethod := myClass compile2: text asString classified: category notifying: requestor							trailer: myClass defaultMethodTrailer ifFail: [^nil].	selector := compiledMethod selector.	self writeSourceToLog.	priorMethodOrNil := myClass compiledMethodAt: selector ifAbsent: [nil].	priorCategoryOrNil := myClass  organization categoryOfElement: selector.