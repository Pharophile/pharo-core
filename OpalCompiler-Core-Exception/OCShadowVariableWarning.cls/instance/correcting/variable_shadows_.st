variable: varNode shadows: semVar	(varNode name = 'self' | varNode name = 'super' )		ifTrue: [ 			^ OCSemanticError new				node: node;				compilationContext: compilationContext;				messageText: self stringMessage;				signal ].	self interactive		ifFalse: [ 			^ Transcript				show: '(';				show: varNode name;				show: ' is shadowed)';				yourself].	self notify: self stringMessage at: varNode start