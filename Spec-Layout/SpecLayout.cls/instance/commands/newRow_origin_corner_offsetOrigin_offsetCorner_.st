newRow: aBlock origin: originPoint corner: cornerPoint offsetOrigin: ooPoint offsetCorner: ocPoint	| block |		block := aBlock.	block isSymbol ifTrue: [ block := [:r | r add: block ] ].		commands add: (SpecLayoutAddRow							block: block							layoutFrame: ((originPoint corner: cornerPoint) asLayoutFrame 										topLeftOffset: ooPoint;										bottomRightOffset: ocPoint)).