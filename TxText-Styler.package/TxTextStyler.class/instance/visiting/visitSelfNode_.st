visitSelfNode: aSelfNode
	| link methodClass |
	
	self 
		addStyle: #self
		forNode: aSelfNode.
	
	methodClass := aSelfNode method methodClass.
		
	link := TxActionAttribute new 
		filter: [ :evt | evt commandKeyPressed and: [ evt  isMouseDown] ];
		value: [ methodClass browse ].

	self 
		addAttribute: link
		forNode: aSelfNode.