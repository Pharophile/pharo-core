requestProtocolNameFor: aClass initialAnswer: aString		| entryCompletion |	entryCompletion := EntryCompletion new		dataSourceBlock: [ :currText | self protocolSuggestionsFor: aClass ];		filterBlock: [ :currApplicant :currText | currApplicant  beginsWith: currText ].	^ UIManager default		request: 'Please type new category name'		initialAnswer: aString		entryCompletion: entryCompletion.