addSpecialFields: elements	elements add: (DynamicEyeElement 			host: self object 			label: 'hex' 			value: [ String streamContents: [ :s | 					s nextPutAll: '['.					self object						do: [ :each | s << (each printPaddedWith: $0 to: 2 base: 16)  ]						separatedBy: [ s nextPut: $ ].					s nextPut: $] ] ] ).			elements add: (DynamicEyeElement 			host: self object 			label: 'string' 			value: [ self object ] ).			