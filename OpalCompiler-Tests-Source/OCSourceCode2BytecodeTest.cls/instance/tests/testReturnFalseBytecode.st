testReturnFalseBytecode	| selector methodNode scanner did |	selector := (methodNode := self compile2methodNode: 'returnFalseBytecode 3 + 4. ^ false') selector.	scanner := InstructionStream on: methodNode method.	4		timesRepeat: [scanner nextInstruction].	self		assert: ((did := scanner peekInstruction) selector == #methodReturnConstant:				and: [did arguments first not])		description: 'Failed ' , selector