testPrimBitAnd	| selector methodNode scanner did |	selector := (methodNode := self compile2methodNode: 'bytecodePrimBitAnd 3 bitAnd: 4') selector.	scanner := InstructionStream on: methodNode method.	2		timesRepeat: [scanner nextInstruction].	self		assert: ((did := scanner peekInstruction) selector == #send:super:numArgs:				and: [did arguments = {#bitAnd:. false. 1}])		description: 'Failed ' , selector