testPrimAtPut	| selector methodNode scanner did |	selector := (methodNode := self compile2methodNode: 'bytecodePrimAtPut self at: 3 put: 4') selector.	scanner := InstructionStream on: methodNode method.	3		timesRepeat: [scanner nextInstruction].	self		assert: ((did := scanner peekInstruction) selector == #send:super:numArgs:				and: [did arguments = {#at:put:. false. 2}])		description: 'Failed ' , selector