testStoreAndPopReceiverVariableBytecode	| selector methodNode scanner did |	selector := (methodNode := self compile2methodNode: 'storeAndPopReceiverVariableBytecode instVar := self') selector.	scanner := InstructionStream on: methodNode method.	self		assert: ((did := scanner nextInstruction; peekInstruction) selector == #popIntoReceiverVariable:				and: [did arguments first == 2])		description: 'Failed ' , selector