testMoveToFailingExistingDestination	| base file folder |		base := filesystem workingDirectory.		folder := (base / 'folder') ensureDirectory. 	(folder / 'newFile') ensureFile.	file := (base / 'file') ensureFile.		"Cleanup after running"	self 		markForCleanup: (base / 'folder' / 'newFile');		markForCleanup: (base / 'folder') ;		markForCleanup: (base / 'file').			"Destination exists already"	self should: [ file moveTo: (folder / 'newFile') ] raise: Error.	self assert: (base / 'file') exists.	self assert: (folder / 'newFile') exists.