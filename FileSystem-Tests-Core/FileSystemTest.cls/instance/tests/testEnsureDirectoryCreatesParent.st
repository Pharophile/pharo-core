testEnsureDirectoryCreatesParent	| path |	path := Path * 'plonk' / 'griffle'.		self markForCleanup: path.	self markForCleanup: path parent.		self 		shouldnt: [filesystem ensureDirectory: path]		raise: FileSystemError.	self assert: (filesystem isDirectory: (Path * 'plonk')).	self assert: (filesystem isDirectory: path).	