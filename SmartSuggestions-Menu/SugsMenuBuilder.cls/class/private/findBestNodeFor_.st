findBestNodeFor: context	| root node|	root := context selectedMethod 		ifNil:[ RBParser parseFaultyExpression: context code] 		ifNotNil: [ RBParser parseFaultyMethod: context code].	context selectedClass ifNotNil: [:theClass| root methodNode methodClass: theClass].	root doSemanticAnalysis.	node := root bestNodeFor: context selectedInterval.	context selectedNode: node.	^node.