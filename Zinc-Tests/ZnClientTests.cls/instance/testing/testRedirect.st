testRedirect	| client response |	self skip.	client := ZnClient new url: 'http:/ci.pharo.org'.	self assert: (client get; isSuccess).	client 		close;		maxNumberOfRedirects: 0;		url: 'http:/ci.pharo.org'.	self should: [ client get ] raise: ZnTooManyRedirects.	client close.	response := [ ZnClient new beOneShot; maxNumberOfRedirects: 0; get: 'http:/ci.pharo.org'; response ]		on: ZnTooManyRedirects do: [ :exception | exception resume ].	self assert: response isRedirect 