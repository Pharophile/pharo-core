initializeTextForToDo	| ruleId versionId manifestBuilder |	manifestBuilder := self builderManifestClass of: critic.	ruleId := rule class uniqueIdentifierName.	versionId := rule class identifierMinorVersionNumber.	(manifestBuilder hasToDoOf: ruleId version: versionId)		ifFalse: [ ^ self ].	commentInputFieldModel		text: ((self builderManifestClass of: critic) commentOfToDo: critic onRule: ruleId version: versionId)