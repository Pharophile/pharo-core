inspectionSubMenu: aMenu	|selectedObject|		selectedObject := self selectedElement object.		aMenu 		add: 'Inspect (i)' translated			target: self			selector: #inspectInNewWindow:			argumentList: {selectedObject};					add: 'Dive (d)' translated			target: self			selector: #dive:			argumentList: {selectedObject}.				mementoPrev size = 0 ifFalse: [ 		 aMenu 			add: 'previous object (p)' translated				target: self				selector: #prev].				mementoNext size = 0 ifFalse: [ 		 aMenu 			add: 'next object (n)' translated				target: self				selector: #next].				aMenu		add: 'Explore (I)' translated			target: self			selector: #explore:			argumentList: {selectedObject}.				aMenu addLine.