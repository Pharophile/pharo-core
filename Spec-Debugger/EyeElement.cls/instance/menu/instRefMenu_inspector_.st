instRefMenu: aMenu inspector: inspector		|class|		class := inspector objectClass.		aMenu 		add: 'Inst var refs...'			target: self			selector: #browseInstVarRefs:			argumentList: {class};		add: 'Inst var defs...'			target: self			selector: #browseInstVarDefs:			argumentList: {class};		add: 'Class var refs...'			target: self			selector: #classVarRefs:			argumentList: {class};		add: 'Class variables'			target: self			selector: #browseClassVariables:			argumentList: {class};		add: 'Class refs (N)'			target: self			selector: #browseClassRefs:			argumentList: {class}.			aMenu addLine.	