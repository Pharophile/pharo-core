referenceSubMenu: aMenu inspector: inspector		|class|		class := inspector objectClass.		class isVariable ifFalse: [ 		aMenu 			add: 'Method refs to this inst var'				target: self 				selector: #referencesToSelection:				argumentList: {class};			add: 'Methods storing into this inst var'				target: self 				selector: #defsOfSelection:				argumentList: {class}.	].	aMenu 		add: 'Explore pointers'			target: self 			selector: #exploreSelection;		add: 'Explore strong pointers'			target: self 			selector: #sxploreStrongPointers.		aMenu addLine.		