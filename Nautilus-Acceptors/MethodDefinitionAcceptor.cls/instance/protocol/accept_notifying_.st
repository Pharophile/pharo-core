accept: source notifying: aController	| protocol method |	protocol := self model selectedCategory.	method := self model selectedMethod.		(protocol isNil or: [ protocol = self model allLabel ])		ifTrue: [			protocol := method 				ifNil: [ Categorizer default ]				ifNotNil: [ method protocol ]] .	self model		compileAMethodFromCategory: protocol		withSource: source		notifying: aController