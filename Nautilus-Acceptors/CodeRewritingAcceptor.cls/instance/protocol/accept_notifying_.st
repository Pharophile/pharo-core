accept: aText notifying: aController	| environment tree rule compiler result |	self model sourceTextArea update: #clearUserEdits.		environment := self model browsedEnvironment.	compiler := Compiler.	tree := compiler evaluate: aText.		rule := CodeRewritingRule new		rewriteRule: tree;		yourself.		result := RBSmalllintChecker runRule: rule onEnvironment: environment.	(ChangesBrowser changes: result builder changes) openWithSpec 