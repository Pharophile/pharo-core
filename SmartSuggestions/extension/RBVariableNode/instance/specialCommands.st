specialCommands	self binding isInstance		ifTrue: [ ^ SugsSuggestionFactory commandsForInstanceVariable].	self binding isTemp		ifTrue: [ ^SugsSuggestionFactory commandsForTemporaryVariable].	(self binding isLiteral and: [Smalltalk globals  includesAssociation: self binding binding ])		ifTrue:[ ^SugsSuggestionFactory commandsForClass].	(self binding isUndeclared )		ifTrue:[^SugsSuggestionFactory commandsForUndeclaredVariable].	(self binding isLiteral and: [(Smalltalk globals  includesAssociation: self binding binding ) not])		ifTrue:[^SugsSuggestionFactory commandsForClassVariable].	^{}.