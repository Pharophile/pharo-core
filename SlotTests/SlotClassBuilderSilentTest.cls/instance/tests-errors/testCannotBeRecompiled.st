testCannotBeRecompiled	| superclass |	superclass := self buildClassWith: [ :builder | 		builder 			superclass: Object;			name: self aClassName ].	self buildClassWith: [ :builder | 		builder 			superclass: superclass;			name: self anotherClassName ].	self 		should: [ 			self buildClassWith: [ :builder | 				builder 					layoutClass: VariableLayout;					name: self aClassName ] ]		raise: Error		description: 'Old class builder raises: X cannot be recompiled'