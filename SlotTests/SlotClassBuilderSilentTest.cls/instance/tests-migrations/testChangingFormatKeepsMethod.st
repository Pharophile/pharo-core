testChangingFormatKeepsMethod	aClass := self buildClassWith: [ :builder | 		builder 			name: self aClassName;			layoutClass: PointerLayout ].	aClass compile: 'a ^self'.	self assert: (aClass methodDictionary includesKey: #a).		anotherClass := self buildClassWith: [ :builder | 		builder 			name: self aClassName;			layoutClass: VariableLayout .			].	self assert: (anotherClass methodDictionary includesKey: #a).