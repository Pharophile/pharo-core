testBlockTemps	| newCompiledMethod originalCompiledMethod |		(Smalltalk hasClassNamed: #Compiler) ifFalse: [^self skip].		newCompiledMethod := (OpalCompiler new 								compile: 'ascentOf: aCharacter											^ [ | temp1 temp2 temp3 | 1= temp1. 2 = temp2. 3 = temp3].'								in: OCMockCompilationClass								classified: nil								notifying: nil								ifFail: [])									generate.	originalCompiledMethod := ((Smalltalk classNamed: #Compiler) new 								compile: 'ascentOf: aCharacter											^ [ | temp1 temp2 temp3 | 1= temp1. 2 = temp2. 3 = temp3].'								in: OCMockCompilationClass								classified: nil								notifying: nil								ifFail: [])									generate.								self assert: newCompiledMethod numArgs = originalCompiledMethod numArgs.	self assert: newCompiledMethod numLiterals = originalCompiledMethod numLiterals.	self assert: newCompiledMethod numTemps = originalCompiledMethod numTemps.	self assert: newCompiledMethod primitive = originalCompiledMethod primitive.	self assert: newCompiledMethod header = originalCompiledMethod header