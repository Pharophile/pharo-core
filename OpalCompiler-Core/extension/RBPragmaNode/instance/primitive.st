primitive	| args module name spec|	args :=  (self arguments			collect: [ :each | each value ]) asArray.	self isPrimitive ifFalse: [OCPrimitiveNode null].		( args first isString )		ifTrue: [name := args first.				 module := args at: 2 ifAbsent: [ nil ].     		spec := (Array 						with: (module ifNotNil: [ module asSymbol ])						with: name asSymbol						with: 0 with: 0).				^OCPrimitiveNode new							num: 117;							spec: spec;							yourself]		ifFalse: [^OCPrimitiveNode new							num: (args first);							spec: nil;							yourself].	