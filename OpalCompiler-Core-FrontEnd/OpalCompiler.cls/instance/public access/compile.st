compile	| cm |	[ast := self parse.	self doSemanticAnalysis.	cm := ast generate ]		on: SyntaxErrorNotification 		do: [ :ex | 			compilationContext requestor                ifNotNil: [ compilationContext requestor notify: ex errorMessage , ' ->' at: ex location in: ex errorCode.                             ^failBlock value]                ifNil: [ ex pass ]].