emitIfNilIfNotNil: aMessageNode	| args |	valueTranslator visitNode: aMessageNode receiver.	args := aMessageNode arguments.	args last arguments ifNotEmpty: [ args last arguments first ocBinding emitStore: methodBuilder ].	methodBuilder pushLiteral: nil.	methodBuilder send: #==.	methodBuilder jumpAheadTo: #notNilCase if: false.	valueTranslator visitInlinedBlockNode: args first.	methodBuilder jumpAheadTo: #end.	methodBuilder jumpAheadTarget: #notNilCase.	valueTranslator visitInlinedBlockNode: args last.	methodBuilder jumpAheadTarget: #end.