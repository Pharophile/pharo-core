emitIfNotNil: aMessageNode	| args |	valueTranslator visitNode: aMessageNode receiver.	args := aMessageNode arguments.	args first arguments ifNotEmpty: [ args first arguments first binding emitStore: methodBuilder ].	methodBuilder pushLiteral: nil.	methodBuilder send: #==.	methodBuilder jumpAheadTo: #notNil if: false.	methodBuilder pushLiteral: nil.	methodBuilder jumpAheadTo: #end.	methodBuilder jumpAheadTarget: #notNil.		self visitInlinedBlockNode: args first.	methodBuilder jumpAheadTarget: #end.