implement: aMessage inClass: aClass		aClass		compile: (DynamicMessageImplementor for: aMessage in: aClass) value		classified: (self askForCategoryIn: aClass default: 'as yet unclassified').		self context privRefreshWith: (aClass lookupSelector: aMessage selector).	self context method numArgs > 0 ifTrue:		[(self context tempAt: 1) arguments withIndexDo:			[:arg :index|			self context tempAt: index put: arg]].		self contextChanged